class Solution {
    public int countPrimeSetBits(int L, int R) {
        int res = 0;
        for(int i = L;i<=R;i++){
            int n = i;
            int setBitNum = 0;
            while(n != 0) {
                if(n%2 != 0) {
                    setBitNum++;
                }
                n >>= 1;
            }
            if(isPrime(setBitNum)) {
                res++;
            }
        }
        return res;
    }
    boolean isPrime(int n){
        int[] primes = {2,3,5,7,11,13,17,19};
        for(int prime: primes) {
            if(n == prime) {
                return true;
            }
        }
        return false;
    }
}