class Solution {
    public int repeatedStringMatch(String A, String B) {
        int cnt = 1;
        String t = A;
        while(t.length() < B.length()) {
            t += A;
            cnt++;
        }
        if(t.indexOf(B) != -1) {
            return cnt;
        } 
        return (t+A).indexOf(B) != -1? cnt+1: -1;
    
    }
}