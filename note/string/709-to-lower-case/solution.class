class Solution {
    public String toLowerCase(String str) {
        char[] chars = str.toCharArray();
        for(int i=0;i<chars.length;i++) {
            if(chars[i] - 'A' >= 0 && 'Z' - chars[i] >= 0 ) {
                chars[i] += 'a' - 'A'; 
            }
        }
        return  String.valueOf(chars);
        // return str.toLowerCase();
    }
}