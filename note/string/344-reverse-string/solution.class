class Solution {
    public String reverseString(String s) {
        StringBuilder sb = new StringBuilder();
        // char[] chars = new char[s.length()];
        for(int i=s.length()-1;i>-1;i--) {
            sb.append(s.charAt(i));
            // chars[s.length()-1-i] = s.charAt(i);
        }
        return sb.toString();
    }
}