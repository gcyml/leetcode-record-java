class Solution {
    public int rotatedDigits(int N) {
        int res= 0;
        for(int i=1;i<=N;i++) {
            if(helper(i)) {
                res++;
            }
        }
        return res;
    }
    public boolean helper(int n) {
        int selfCount = 0;
        int count = 0;
        while(n != 0) {
            int tmp = n%10;
            if(tmp == 3 || tmp ==4 || tmp == 7) {
                return false;
            } else if(tmp == 0 || tmp ==1 || tmp == 8){
                selfCount++;
            }
            n /= 10;
            count++;
        }
        if(selfCount == count) {
            return false;
        }
        return true;
    }
}