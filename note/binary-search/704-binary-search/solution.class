class Solution {
    public int search(int[] nums, int target) {
        int low = 0, hight = nums.length-1;
        while(low <= hight) {
            int mid = (low + hight)/2;
            if(target > nums[mid]) {
                low = mid+1;
            } else if(target < nums[mid]){
                hight = mid-1;
            } else {
                return mid;
            }
        }
        return -1;
    }
}