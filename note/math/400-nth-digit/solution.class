class Solution {
    public int findNthDigit(int n) {
        int len = 1, start = 1;
        long cnt = 9;
        while(n > len * cnt) {
            n -= len * cnt;
            len++;
            start *= 10;
            cnt *= 10;
        }
        start += (n-1) / len;
        return String.valueOf(start).charAt((n-1)%len) - '0';
    }
}