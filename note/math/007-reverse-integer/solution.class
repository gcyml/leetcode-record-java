class Solution {
    public int reverse(int x) {
        long tmp = x;
        // é²æ­¢ç»ææº¢åº
        long result = 0;

        while (tmp != 0) {
            result = result * 10 + tmp % 10;
            tmp = tmp / 10;
        }

        // æº¢åºå¤æ­ 
        if (result < Integer.MIN_VALUE || result > Integer.MAX_VALUE) {
            result = 0;
        }

        return (int) result;
    }
}