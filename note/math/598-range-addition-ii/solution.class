class Solution {
    public int maxCount(int m, int n, int[][] ops) {
        if(ops.length == 0){
            return m*n;
        }
        int minA = Integer.MAX_VALUE, minB = Integer.MAX_VALUE;
        for(int i = 0;i<ops.length;i++) {
            minA = Math.min(ops[i][0], minA);
            minB = Math.min(ops[i][1], minB);
        }
        return minA * minB;
    }
}