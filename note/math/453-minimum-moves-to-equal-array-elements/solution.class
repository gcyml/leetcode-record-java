class Solution {
    public int minMoves(int[] nums) {
        // n-1 ä¸ªæ°+1ä½¿å¾æåç¸ç­ï¼ç­ä»·äºæ¯æ¬¡ç§»å¨ä¸ä¸ªæ°å1ï¼ä½¿å¾æåç¸ç­
        int mn = Integer.MAX_VALUE, sum = 0, res = 0;
        for (int num : nums) {
            mn = Math.min(mn, num);
            sum += num;
        }
        return sum - mn * nums.length;
    }
}