class Solution {
    public int[] findErrorNums(int[] nums) {
        int[] res = new int[2];
        boolean[] tmp = new boolean[nums.length];
        for(int i = 0;i<nums.length;i++) {
            if(!tmp[nums[i]-1]) {
                tmp[nums[i]-1] = true;
            } else {
                res[0] = nums[i];
            }
        }
        for(int i = 0;i<tmp.length;i++) {
            if(!tmp[i]) {
                res[1] = i+1;
                break;
            }
        }
        return res;
    }
}