class Solution {
    public int distributeCandies(int[] candies) {
        HashSet<Integer> set = new HashSet<>();
        int half = candies.length/2;
        for(int candie: candies) {
            set.add(candie);
            if(set.size() > half) {
                return half;
            }
        }
        return set.size();
    }
}