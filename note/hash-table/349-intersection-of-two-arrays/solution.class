class Solution {
    public int[] intersection(int[] nums1, int[] nums2) {
        HashSet<Integer> set = new HashSet<>();
        HashSet<Integer> ans = new HashSet<>();
        for(int num : nums1) {
            set.add(num);
        }
        for(int num : nums2) {
            if(set.contains(num)) {
                ans.add(num);
                // set.remove
            }
        }
        //éålistæä¸ºæ°ç»è¿å
        int[] a = new int[ans.size()];
        int index = 0;
        Iterator<Integer> i= ans.iterator();
        while(i.hasNext()){
            a[index++] = (int)i.next();
        }

        return a;
    }
}